doctype html
html
    head
        title GetResponse ESLint Rules
        link(rel="icon", type="image/png", sizes="184x184", href="data:image/png;base64,")
        style.
            html, body {
                margin: 0;
                padding: 0;
            }

            h1 {
                font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
                font-size: 24px;
                font-style: normal;
                font-variant: normal;
                font-weight: 700;
                line-height: 26.4px;
            }

            h3 {
                font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
                font-size: 14px;
                font-style: normal;
                font-variant: normal;
                font-weight: 700;
                line-height: 15.4px;
            }

            p, body {
                font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
                font-size: 14px;
                font-style: normal;
                font-variant: normal;
                font-weight: 400;
                line-height: 20px;
            }

            blockquote {
                font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
                font-size: 21px;
                font-style: normal;
                font-variant: normal;
                font-weight: 400;
                line-height: 30px;
            }

            pre {
                font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
                font-size: 13px;
                font-style: normal;
                font-variant: normal;
                font-weight: 400;
                line-height: 18.5714px;
            }

            td, th {
                padding: 10px 20px;
            }

            h1, h2, h3, h4, h5, h6 {
                text-align: center;
                position: sticky;
                top: 0;
                background: #fff;
                padding: .83em;
                margin: 0;
            }

            table {
                border-spacing: 0;
                border-collapse: collapse;
            }

            thead tr {
                background: #00baff;
            }

            th {
                color: #fff;
                font-weight: 700;
                letter-spacing: .02em;
            }

            .name {
                font-weight: 500;
            }

            table {
                width: 100%;
            }

            tbody tr:nth-child(even) {
                background: #f6f3f3;
            }

            .status {
                text-align: center;
            }

            .status.warn {
                color: #e4762d;
            }

            .status.error {
                color: #e5101e;
            }

            .active-rules thead tr {
                position: sticky;
                top: 60px;
            }

            tbody tr:hover {
                background: #ebfaff;
            }

    body
        mixin name(rule)
            if rule.url
                a.name(href=rule.url, target="_blank")= rule.name
            else
                span.name= rule.name
            if rule.description
                div!= rule.description

        if deprecated.length > 0
            section
                h1 Deprecated rules
                table
                    tbody
                        each rule in deprecated
                            tr
                                td
                                    +name(rule)
        section.active-rules
            h1 Active rules
            table
                thead
                    tr
                        th Rule
                        th Status
                        th Configuration
                tbody
                    each rule in enabled
                        tr
                            td
                                +name(rule)
                            td.status(class=rule.status)= rule.status
                            td
                                pre= rule.config ? JSON.stringify(rule.config, null, '\t') : ' - '
        if unused.length > 0
            section
                h1 Unused rules
                table
                    tbody
                        each rule in unused
                            tr
                                td
                                    +name(rule)
